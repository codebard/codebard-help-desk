<div class="{***prefix***}user_ticket_listing_entry {***newly_updated***}" onclick="window.location.href = '{***ticket_link***}'">

	<div class="{***prefix***}user_ticket_listing_upper_row_wrapper">
		<div class="{***prefix***}user_ticket_listing_ticket_title">
			<a href="{***ticket_link***}" class="{***prefix***}user_ticket_listing_ticket_title_link">{***ticket_title***}</a>
			<div class="{***prefix***}user_ticket_listing_ticket_info">
				{***user_ticket_last_updated_by***} <b>	{***last_updated_by***}</b>&nbsp;
			</div>			
		</div>
		<div class="{***prefix***}user_ticket_listing_view_ticket_button">
			<button class="button" onclick="window.location='{***ticket_link***}';">{***button_label_view_or_resolved***}</button>
		</div>
	</div>
	
</div>